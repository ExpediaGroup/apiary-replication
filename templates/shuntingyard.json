[
  {
    "name": "shuntingyard",
    "image": "${docker_image}:${docker_version}",
    "essential": true,
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${loggroup}",
            "awslogs-region": "${region}",
            "awslogs-stream-prefix": "/"
        }
    },
    "portMappings": [
      {
        "containerPort": 48869,
        "hostPort": 48869
      }
    ],
    "environment":[
     {
        "name": "HEAPSIZE",
        "value": "${heapsize}"
     },
     {
        "name": "SHUNTINGYARD_CONFIG_YAML",
        "value": "${shuntingyard_config_yaml}"
     }
    ]
  }
]
